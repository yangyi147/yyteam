<!DOCTYPE mapper   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
<mapper namespace="com.mapper.Edu_ClassDao">
<select id="getlistAll" resultType="EClass">
   select * from edu_class
</select>
